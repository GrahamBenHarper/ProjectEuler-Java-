public class Problem13
{
    public static void main(String[] args) throws FileNotFoundException
    {
        
        
        
    }
    
    public static int[] importNumbersToAdd() throws FileNotFoundException // Loads the 100 50-digit numbers that were given
    {
        Scanner input = new Scanner(new File("Problem13.txt"));
        Long[] x = new Long[100];
        int i=0;
        
        while(input.hasNext())
        {
            x[i]=Integer.valueOf(input.next());
            i++;
        }
        
        return x;
    }
    
    public static Long addBigNumbers(Long[] x)
    {
        Long sum = 0;
        
        for(int i=0; i<x.length; i++)
        {    
            sum+=x[i];
        }
    }
}